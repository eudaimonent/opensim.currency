<?xml version="1.0" encoding="utf-8" ?>
<Prebuild xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.7.xsd" version="1.7">
  <Solution name="OpenSim.Currency" activeConfig="Release" path="./" version="0.5.0-$Rev$">
    <Configuration name="Debug">
      <Options>
        <CompilerDefines>TRACE;DEBUG</CompilerDefines>
        <OptimizeCode>false</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>false</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings></SuppressWarnings>
        <OutputPath>bin</OutputPath>
        <DebugInformation>true</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>
    <Configuration name="Release">
      <Options>
        <CompilerDefines>TRACE</CompilerDefines>
        <OptimizeCode>true</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>false</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings></SuppressWarnings>
        <OutputPath>bin</OutputPath>
        <DebugInformation>false</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>

    <Project name="OpenSim.Data.MySQL.MySQLMoneyDataWrapper" path="." type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>./bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>./bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../bin/</ReferencePath>
      <Reference name="System" localCopy="false"/>
      <Reference name="System.Xml"/>
      <Reference name="System.Data"/>
      <Reference name="System.Drawing"/>
      <Reference name="System.Runtime.Remoting"/>
      <Reference name="Axiom.MathLib.dll"/>
      <Reference name="Nini.dll" />
      <Reference name="log4net.dll"/>
      <Reference name="XMLRPC.dll"/>
      <Reference name="MySql.Data"/>
      <Reference name="Mono.Addins.dll"/>
      <Reference name="OpenMetaverseTypes.dll"/>
      <Reference name="OpenMetaverse.dll"/>
      <Reference name="OpenSim.Region.Framework" />
      <Reference name="OpenSim.Framework"/>
      <Reference name="OpenSim.Framework.Console"/>
      <Reference name="OpenSim.Framework.Servers"/>
      <Reference name="OpenSim.Framework.Servers.HttpServer"/>
      <Reference name="OpenSim.Framework.Communications"/>
      <Reference name="OpenSim.Region.Physics.Manager"/>
      <Reference name="OpenSim.Services.Interfaces"/>
      <Reference name="OpenSim.Server.Base"/>
      <Reference name="OpenSim.Data" />
      <Files>
        <Match path="OpenSim.Data.MySQL.MySQLMoneyDataWrapper" pattern="*.cs" recurse="true"/>
      </Files>
    </Project>


    <Project name="OpenSim.Modules.Currency" path="." type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>./bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>./bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../bin/</ReferencePath>
      <Reference name="System" localCopy="false"/>
      <Reference name="System.Xml"/>
      <Reference name="System.Drawing"/>
      <Reference name="System.Runtime.Remoting"/>
      <Reference name="Axiom.MathLib.dll"/>
      <Reference name="Nini.dll" />
      <Reference name="log4net.dll"/>
      <Reference name="XMLRPC.dll"/>
      <Reference name="Mono.Addins.dll"/>
      <Reference name="OpenMetaverseTypes.dll"/>
      <Reference name="OpenMetaverse.dll"/>
      <Reference name="OpenSim.Region.Framework" />
      <Reference name="OpenSim.Framework"/>
      <Reference name="OpenSim.Framework.Console"/>
      <Reference name="OpenSim.Framework.Servers"/>
      <Reference name="OpenSim.Framework.Servers.HttpServer"/>
      <Reference name="OpenSim.Framework.Communications"/>
      <Reference name="OpenSim.Region.Physics.Manager"/>
      <Reference name="OpenSim.Services.Interfaces"/>
      <Reference name="OpenSim.Server.Base"/>
      <Reference name="OpenSim.Data"/>
      <Files>
        <Match path="OpenSim.Modules.Currency" pattern="*.cs" recurse="true"/>
        <Match path="OpenSim.Grid.MoneyServer" pattern="NSLXmlRpc.cs" recurse="false"/>
      </Files>
    </Project>


    <Project name="MoneyServer" path="." type="Exe">
      <Configuration name="Debug">
        <Options>
          <OutputPath>./bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>./bin/</OutputPath>
        </Options>
      </Configuration>
      <ReferencePath>../bin/</ReferencePath>
      <Reference name="System"/>
      <Reference name="System.Core"/>
      <Reference name="System.Xml"/>
      <Reference name="System.Data"/>
      <Reference name="System.Drawing"/>
      <Reference name="System.Runtime.Remoting"/>
      <Reference name="Axiom.MathLib.dll"/>
      <Reference name="Nini.dll" />
      <Reference name="log4net.dll"/>
      <Reference name="XMLRPC.dll"/>
      <Reference name="MySql.Data"/>
      <Reference name="Mono.Addins.dll"/>
      <Reference name="OpenMetaverseTypes.dll"/>
      <Reference name="OpenMetaverse.dll"/>
      <Reference name="OpenSim.Region.Framework" />
      <Reference name="OpenSim.Framework"/>
      <Reference name="OpenSim.Framework.Console"/>
      <Reference name="OpenSim.Framework.Servers"/>
      <Reference name="OpenSim.Framework.Servers.HttpServer"/>
      <Reference name="OpenSim.Framework.Communications"/>
      <Reference name="OpenSim.Region.Physics.Manager"/>
      <Reference name="OpenSim.Services.Interfaces"/>
      <Reference name="OpenSim.Server.Base"/>
      <Reference name="OpenSim.Data" />
      <Reference name="OpenSim.Data.MySQL.MySQLMoneyDataWrapper" path="./bin/"/>
      <Reference name="OpenSim.Modules.Currency" path="./bin/"/>
      <Files>
        <Match path="OpenSim.Grid.MoneyServer" pattern="*.cs" recurse="true"/>
      </Files>
    </Project>


  </Solution>
</Prebuild>

